  <!--[if lt IE 9]>
      <script src="http://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7/html5shiv.js"></script>
  <![endif]-->
  
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  
  <script src="http://use.typekit.net/gis6vng.js"></script>
  <script>
      try { Typekit.load(); } catch (e) {}
  </script>


    
  <script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-41480445-1', 'purecss.io');
  ga('send', 'pageview');
  </script>

  
  
  <%= csrf_meta_tags %>
</head>
  <body>
    <div id="layout">
        <a href="#menu" id="menuLink" class="menu-link">
    <span></span>
</a>

<div id="menu">
    <div class="pure-menu pure-menu-open">
        <a class="pure-menu-heading" href="/">Notebook</a>

        <ul>
          
            <li class="menu-item-divided ">
                <a href="/"><i class="fa fa-home"></i>&nbsp;Home</a>
            </li>
          
          
            <li class="menu-item-divided">
				<%= link_to swimmers_path do %>
					<i class="fa fa-rocket"></i>&nbsp;Swimmers
				<% end %>
            </li>
          
            <li class=" ">
				<%= link_to events_path do %>
					<i class="fa fa-calendar"></i>&nbsp;Events
				<% end %>
				
            </li>
          
            <!-- <li class=" pure-menu-selected"> -->
			<li>
                <a href="/settings/"><i class="fa fa-gear"></i> Settings</a>
            </li>
          
        </ul>
    </div>
</div>


        <div id="main">


<div class="content">
      <div class="banner sixteen columns clearfix">
	<% if flash[:notice] %>
	<p class="notice"><%= flash[:notice] %></p>
	<% end %>
	<% if flash[:error] %>
	<p class="error"><%= flash[:error] %></p>
	<% end %>
      </div>
      <div class="sidebar one columns">
	<nav>
	  <ul>
		  
	    <li><i class="fa fa-camera-retro"></i>&nbsp;<%= link_to "New effort...", new_effort_path %></li>
	  </ul>
	</nav>
      </div>
      <div class="content ten columns">
	<%= yield %>
      </div>
    </div>
            <div class="footer">
                <div class="legal pure-g">
    <div class="pure-u-1 pure-u-sm-1-2">
        <p class="legal-license">
            This site is built with &lt;3 using Pure v0.4.2<br>
